test "example" {
  case "ok" {
    out = trimspace(<<EOS
somemap_deprecated.foo=FOO
somemap_deprecated.bar=BAR
somemap_deprecated["foo"]=FOO
somemap_deprecated["bar"]=BAR

somemap.foo=FOO
somemap.bar=BAR
somemap["foo"]=FOO
somemap["bar"]=BAR

emptymap.foo=FOO

sometuple.0=x
sometuple.1=1
sometuple[0]=x
sometuple[1]=1

someobj.foo=FOO
someobj.bar=1
someobj["foo"]=FOO
someobj["bar"]=1
EOS
    )
    exitstatus = 0
  }

  run "example" {
  }

  assert "out" {
    condition = run.res.stdout == case.out
  }

  assert "exitstatus" {
    condition = run.res.exitstatus == case.exitstatus
  }
}
